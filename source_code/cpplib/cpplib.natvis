    <DisplayString Condition="std::holds_alternative&lt;Bye&gt;(m_data)">[Bye]</DisplayString>
  </Type>
  <Type Name="tournament_builder::Competition">
    <DisplayString Condition="std::holds_alternative&lt;tournament_builder::RealCompetition&gt;(m_data)">{std::get&lt;tournament_builder::RealCompetition&gt;(m_data)}</DisplayString>
    <DisplayString Condition="std::holds_alternative&lt;tournament_builder::descriptor::DescriptorHandle&gt;(m_data)">{std::get&lt;tournament_builder::descriptor::DescriptorHandle&gt;(m_data)}</DisplayString>
