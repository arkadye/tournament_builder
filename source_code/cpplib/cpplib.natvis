    <DisplayString Condition="true">{m_data}</DisplayString>
  </Type>
  <Type Name="tournament_builder::NamedElement">
    <DisplayString>{name}</DisplayString>
  </Type>
  <Type Name="tournament_builder::Tag">
    <DisplayString>{to_string()}</DisplayString>
  </Type>
  <Type Name="tournament_builder::SoftReference">
    <DisplayString>{to_string()}</DisplayString>
  </Type>
  <Type Name="tournament_builder::internal_reference::ReferenceBase">
    <DisplayString>{to_string()}</DisplayString>
  </Type>
